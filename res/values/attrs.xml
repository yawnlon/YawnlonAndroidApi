<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- YTtfTextView -->
    <declare-styleable name="YTtfTextView">
        <attr name="font_path" format="string|reference" />
        <attr name="font_path_b" format="string|reference" />
    </declare-styleable>

    <!-- CircleImageView -->
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_alpha" format="integer" />
    </declare-styleable>

</resources>